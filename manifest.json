{
  "name": "Yon URL Shortener",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Shorten your long URLs with Yon and track the clickers",
  "browser_action": {
    "default_icon": "icon.png",
	"default_popup": "popup.html"
  },
  
  "icons": { 
    "16": "16.png",
    "48": "48.png",
    "128": "128.png"
  }, 
 
  "permissions": [ "http://*/*", "https://*/*", "tabs", "notifications", "contextMenus", "unlimitedStorage", "clipboardWrite" ]
}